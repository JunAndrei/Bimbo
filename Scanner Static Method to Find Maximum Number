package com.mycompany.teambim;
import java.util.Scanner;
public class TeamBim {
     public static int max(int a, int b){
         return (a > b) ? a : b;
     }
     
     public static void main (String[]args){
         Scanner TeamBim = new Scanner(System.in);
        System.out.println("Enter 1st number");
        int num1 = TeamBim.nextInt();
        System.out.println("Enter 2nd number");
        int num2 = TeamBim.nextInt();
        
         int maxNumber = max(num1, num2);
         System.out.println("Maximum number between " + num1 + " and " + num2 + " is: " + maxNumber);
     }
   }
